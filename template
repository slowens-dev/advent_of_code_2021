#include<stdlib.h>
#include<stdio.h>

#define FILEPATH "input/dayn.txt"

int main(){
	FILE *fp = fopen( FILEPATH, "r");
	char* linebuf;
	size_t linebuf_sz;
	int line_len;

	for( line_len = getline( &linebuf, &linebuf_sz, fp); line_len >= 0; line_len = getline( &linebuf, &linebuf_sz, fp)){
		printf("%s", linebuf); //the work
	}

	fclose( fp );
	free(linebuf);
}


#ifndef __AOCdayn_H_
#define __AOCdayn_H_

int aocdayn( char* filepath );
int aocdayn_2( char* filepath );

#endif



#include<stdlib.h>
#include<stdio.h>

#include "../aoc1.h"

int main(){
	printf("dayn.1\t:\t%d\n", aocdayn("input/dayn.txt"));
	printf("dayn.2\t:\t%d\n", aocdayn_2("input/dayn.txt"));
}
